package com.aurionpro.homework;

public class homework {

	public static void main(String[] args) {
int[]hello;
hello=new int[10];
hello[0]=1;
hello[1]=2;
hello[2]=3;
hello[3]=4;
hello[4]=5;
hello[5]=6;
hello[6]=7;
hello[7]=8;
hello[8]=9;
hello[9]=10;
hello[10]=11;
for(int i=0;i<hello.length;i++) {
System.out.println("Element of Index"+i+":"+hello[i]);
}
}

}
